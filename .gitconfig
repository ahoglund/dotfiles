[user]
  name = Andrew Hoglund
	email = 
[github]
  user = ahoglund
[push]
  default = simple
[credential]
  helper = osxkeychain
[alias]
  st = status
  df = diff
  ci = commit
  co = checkout
  br = branch
  pl = pull
  ps = push
  evolve = "! f() { git checkout $1 && git pull;}; f"
[init]
  templatedir = ~/.git_template
[url "git@github.com:"]
  insteadOf = https://github.com/
[credential]
  helper = cache --timeout=3600
[gpg]
	program = gpg2
